<div class="panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title" align="center">Files</h3>
  </div>
  <div class="panel-body">
    {{#each files}}
    <li class="list-group-item"><span class="label label-success pull-right">{{this.repeat}}</span>{{this.id}}</li>
    {{/each}}
  </div>
</div>
